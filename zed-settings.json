      "command": "/Users/ar667356/Personal/google-cloud-mcp-python/.venv/bin/python",
      "args": [
        "/Users/ar667356/Personal/google-cloud-mcp-python/src/main.py",
        "--transport",
        "stdio",
      ],
      "env": null,
    },
    "playwright": {
      "enabled": false,
      "command": "npx",
      "args": ["@playwright/mcp@latest", "--browser", "chrome", "--isolated"],
    },
    "mcpServers": {
      "settings": {},
    },
  },
  "ssh_connections": [
    {
      "host": "pi",
      "args": [],
      "projects": [
        {
          "paths": ["/home/kannie"],
        },
      ],
    },
    {
      "host": "pizza",
      "args": [],
      "projects": [],
    },
  ],
  "icon_theme": "Catppuccin Mocha",
  "base_keymap": "VSCode",
  "buffer_font_family": "Fira Code",
  "buffer_font_size": 13,
  "buffer_font_weight": 400,
  "buffer_line_height": {
    "custom": 1.45,
  },
  "ui_font_family": "Fira Code",
  "ui_font_size": 13,
  "ui_font_weight": 500,
  "soft_wrap": "editor_width",
  "theme": {
    "mode": "system",
    "light": "Catppuccin Latte",
    "dark": "Catppuccin Mocha",
  },
  "lsp": {
    "ty": {
      "settings": {},
    },
    "rust-analyzer": {
      "initialization_options": {
        "inlayHints": {
          "maxLength": null,
          "lifetimeElisionHints": {
            "enable": "skip_trivial",
            "useParameterNames": true,
          },
          "closureReturnTypeHints": {
            "enable": "always",
          },
        },
      },
    },
    "terraform-ls": {
      "initialization_options": {
        "experimentalFeatures": {
          "prefillRequiredFields": true,
        },
      },
    },
    "texlab": {
      "settings": {
        "texlab": {
          "build": {
            "onSave": true,
            "forwardSearchAfter": true,
          },
          "forwardSearch": {
            "executable": "zathura",
            "args": [
              "--synctex-forward",
              "%l:1:%f",
              "-x",
              "zed %%{input}:%%{line}",
              "%p",
            ],
          },
          "latexFormatter": "latexindent", // default value (this line not necessary)
          "latexindent": {
            // instead of explicitly setting CLI args directly, configure behaviour here
            "modifyLineBreaks": true,
            "local": "path/to/config",
            "replacement": null, // place for flags like -rr -rv -r
          },
        },
      },
    },
  },
  "languages": {
    "Python": {
      "language_servers": ["ty", "ruff", "!basedpyright"],
      "format_on_save": "on",
      "formatter": [
        {
          "language_server": {
            "name": "ruff",
          },
        },
        { "code_action": "source.fixAll.ruff" },
        { "code_action": "source.organizeImports.ruff" },
      ],
      "remove_trailing_whitespace_on_save": true,
      "use_auto_surround": true,
      "use_autoclose": true,
      "show_completions_on_input": true,
      "enable_language_server": true,
      "code_actions_on_format": {
        "source.organizeImports.ruff": true,
      },
    },
    "SQL": {
      "format_on_save": "on",
    },
    "LaTeX": {
      "formatter": {
        "external": {
          "command": "latexindent", // or choose specific path
          "arguments": ["--modifylinebreaks"], // control behaviour with flags
        },
      },
      "format_on_save": "on",
    },
  },
  "autosave": {
    "after_delay": {
      "milliseconds": 500,
    },
  },
  "terminal": {
    "alternate_scroll": "off",
    "blinking": "terminal_controlled",
    "copy_on_select": false,
    "dock": "bottom",
    "default_width": 640,
    "default_height": 320,
    "detect_venv": {
      "on": {
        "directories": [".env", "env", ".venv", "venv"],
        "activate_script": "default",
      },
    },
    "env": {},
    "font_family": "Fira Code",
    "font_features": null,
    "font_size": 12,
    "line_height": "comfortable",
    "option_as_meta": false,
    "button": true,
    "shell": "system",
    "toolbar": {
      "breadcrumbs": true,
    },
    "working_directory": "current_project_directory",
    "scrollbar": {
      "show": null,
    },
  },
  "agent_ui_font_size": 15,
  "agent": {
    "inline_assistant_model": {
      "provider": "google",
      "model": "gemini-2.5-pro",
    },
    "default_model": {
      "provider": "google",
      "model": "gemini-3-pro-preview",
    },
    "model_parameters": [],
  },
  "tabs": {
    "file_icons": true,
    "show_diagnostics": "all",
  },
  "extend_comment_on_newline": false,
  "auto_update": true,
  "lsp_document_colors": "background",
  "file_scan_exclusions": [
    "**/.git",
    "**/.svn",
    "**/.hg",
    "**/.jj",
    "**/CVS",
    "**/.DS_Store",
    "**/Thumbs.db",
    "**/.classpath",
    "**/.settings",
  ],
  "language_models": {
    "bedrock": {
      "authentication_method": "named_profile",
      "region": "ap-southeast-1",
      "profile": "ccoe-center-bedrock",
    },
    "google": {
      "available_models": [
        {
          "name": "gemini-3-pro-preview",
          "display_name": "gemini-3.0-pro",
          "max_tokens": 200000,
          "mode": {
            "type": "thinking",
          },
        },
        {
          "name": "gemini-2.5-pro",
          "display_name": "gemini-2.5-pro",
          "max_tokens": 200000,
          "mode": {
            "type": "thinking",
          },
        },
        {
          "name": "gemini-2.5-flash",
          "display_name": "gemini-2.5-flash",
          "max_tokens": 50000,
          "mode": {
            "type": "thinking",
          },
        },
      ],
    },
    "openai": {
      "api_url": "http://192.168.1.2:4001/v1",
      "available_models": [
        {
          "name": "gpt-4.1",
          "display_name": "litellm-gpt-4.1",
          "max_tokens": 1000000,
        },
        {
          "name": "gpt-5",
          "display_name": "litellm-gpt-5",
          "max_tokens": 1000000,
        },
      ],
    },
  },
  "file_types": {
    "Helm": [
      "**/templates/**/*.tpl",
      "**/templates/**/*.yaml",
      "**/templates/**/*.yml",
      "**/helmfile.d/**/*.yaml",
      "**/helmfile.d/**/*.yml",
      "**/values*.yaml",
    ],
  },
}
