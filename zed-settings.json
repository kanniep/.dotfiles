{
  "context_servers": {
    "google-cloud-cmp-python": {
      "source": "custom",
      "command": "/Users/ar667356/Personal/google-cloud-mcp-python/.venv/bin/python",
      "args": [
        "/Users/ar667356/Personal/google-cloud-mcp-python/src/main.py",
        "--transport",
        "stdio"
      ],
      "env": null
    },
    "playwright": {
      "source": "custom",
      "command": "npx",
      "args": ["@playwright/mcp@latest", "--browser", "chrome", "--isolated"]
    },
    "mcpServers": {
      "source": "extension",
      "settings": {}
    }
  },
  "ssh_connections": [
    {
      "host": "pi",
      "projects": [
        {
          "paths": ["/home/kannie"]
        }
      ]
    }
  ],
  "icon_theme": "Catppuccin Mocha",
  "base_keymap": "VSCode",
  "features": { "edit_prediction_provider": "zed" },
  "buffer_font_family": "Fira Code",
  "buffer_font_size": 13,
  "buffer_font_weight": 400,
  "buffer_line_height": {
    "custom": 1.45
  },
  "ui_font_family": "Fira Code",
  "ui_font_size": 13,
  "ui_font_weight": 500,
  "soft_wrap": "editor_width",
  "theme": {
    "mode": "system",
    "light": "Catppuccin Latte",
    "dark": "Catppuccin Mocha"
  },
  "lsp": {
    "pyright": {
      "settings": {
        "python.analysis": {
          "diagnosticMode": "openFilesOnly",
          "typeCheckingMode": "basic"
        },
        "python": {
          "pythonPath": ".venv/bin/python"
        }
      }
    },
    "taplo": {
      "settings": {
        "array_auto_collapse": false
      }
    },
    "rust-analyzer": {
      "initialization_options": {
        "inlayHints": {
          "maxLength": null,
          "lifetimeElisionHints": {
            "enable": "skip_trivial",
            "useParameterNames": true
          },
          "closureReturnTypeHints": {
            "enable": "always"
          }
        }
      }
    },
    "terraform-ls": {
      "initialization_options": {
        "experimentalFeatures": {
          "prefillRequiredFields": true
        }
      }
    }
  },
  "languages": {
    "Python": {
      "language_servers": ["pyright", "ruff"],
      "format_on_save": "on",
      "formatter": [
        {
          "language_server": {
            "name": "ruff"
          }
        },
        {
          "code_actions": {
            "source.fixAll.ruff": true,
            "source.organizeImports.ruff": true
          }
        }
      ],
      "remove_trailing_whitespace_on_save": true,
      "use_auto_surround": true,
      "use_autoclose": true,
      "show_completions_on_input": true,
      "enable_language_server": true
    },
    "SQL": {
      "format_on_save": "on"
    }
  },
  "autosave": {
    "after_delay": {
      "milliseconds": 500
    }
  },
  "terminal": {
    "alternate_scroll": "off",
    "blinking": "terminal_controlled",
    "copy_on_select": false,
    "dock": "bottom",
    "default_width": 640,
    "default_height": 320,
    "detect_venv": {
      "on": {
        "directories": [".env", "env", ".venv", "venv"],
        "activate_script": "default"
      }
    },
    "env": {},
    "font_family": "Fira Code",
    "font_features": null,
    "font_size": 12,
    "line_height": "comfortable",
    "option_as_meta": false,
    "button": true,
    "shell": "system",
    "toolbar": {
      "breadcrumbs": true
    },
    "working_directory": "current_project_directory",
    "scrollbar": {
      "show": null
    }
  },
  "agent_font_size": 15,
  "agent": {
    "commit_message_model": {
      "provider": "google",
      "model": "gemini-2.5-flash"
    },
    "inline_assistant_model": {
      "provider": "google",
      "model": "gemini-2.5-flash"
    },
    "single_file_review": true,
    "always_allow_tool_actions": true,
    "stream_edits": true,
    "enable_feedback": false,
    "default_profile": "write",
    "profiles": {
      "write": {
        "name": "Write",
        "tools": {
          "open": true,
          "terminal": true,
          "batch_tool": true,
          "code_actions": true,
          "code_symbols": true,
          "contents": true,
          "copy_path": true,
          "delete_path": true,
          "diagnostics": true,
          "edit_file": true,
          "fetch": true,
          "list_directory": true,
          "move_path": true,
          "now": true,
          "find_path": true,
          "read_file": true,
          "grep": true,
          "rename": true,
          "symbol_info": true,
          "thinking": true,
          "web_search": true,
          "create_directory": true,
          "create_file": true
        },
        "enable_all_context_servers": true,
        "context_servers": {}
      }
    },
    "default_model": {
      "provider": "amazon-bedrock",
      "model": "claude-4-sonnet-thinking"
    },
    "notify_when_agent_waiting": "primary_screen",
    "play_sound_when_agent_done": true
  },
  "tabs": {
    "file_icons": true,
    "show_diagnostics": "all"
  },
  "extend_comment_on_newline": false,
  "auto_update": true,
  "lsp_document_colors": "background",
  "file_scan_exclusions": [
    "**/.git",
    "**/.svn",
    "**/.hg",
    "**/.jj",
    "**/CVS",
    "**/.DS_Store",
    "**/Thumbs.db",
    "**/.classpath",
    "**/.settings"
  ],
  "language_models": {
    "bedrock": {
      "authentication_method": "named_profile",
      "region": "ap-southeast-1",
      "profile": "ccoe-center-bedrock"
    },
    "google": {
      "available_models": [
        {
          "name": "gemini-2.5-pro",
          "display_name": "gemini-2.5-pro",
          "max_tokens": 200000,
          "mode": {
            "type": "thinking"
          }
        },
        {
          "name": "gemini-2.5-flash",
          "display_name": "gemini-2.5-flash",
          "max_tokens": 50000,
          "mode": {
            "type": "thinking"
          }
        }
      ]
    },
    "openai": {
      "api_url": "http://192.168.1.2:4001/v1",
      "available_models": [
        {
          "name": "gpt-4.1",
          "display_name": "litellm-gpt-4.1",
          "max_tokens": 1000000
        },
        {
          "name": "gpt-4.1-mini",
          "display_name": "litellm-gpt-4.1-mini",
          "max_tokens": 1000000
        },
        {
          "name": "gpt-4.1-nano",
          "display_name": "litellm-gpt-4.1-nano",
          "max_tokens": 1000000
        }
      ]
    }
  },
  "file_types": {
    "Helm": [
      "**/templates/**/*.tpl",
      "**/templates/**/*.yaml",
      "**/templates/**/*.yml",
      "**/helmfile.d/**/*.yaml",
      "**/helmfile.d/**/*.yml",
      "**/values*.yaml"
    ]
  }
}
